{
  "name": "esports-daily-backend",
  "version": "1.0.0",
  "description": "server side for esports daily",
  "scripts": {
    "start": "nodemon -e js,graphql -x node -r dotenv/config src/index.js dotenv_config_path=variables.env",
    "dev": "nodemon -e js,graphql -x node --inspect -r dotenv/config src/index.js dotenv_config_path=variables.env",
    "test": "jest",
    "deploy": "prisma deploy --env-file variables.env"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jcolborn-dropdeadgames/esports-daily"
  },
  "author": "Jacob Colborn",
  "license": "ISC",
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "2.4.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "6.0.0",
    "graphql": "^0.13.2",
    "graphql-cli": "^2.16.7",
    "graphql-yoga": "1.17.4",
    "jsonwebtoken": "9.0.0",
    "nodemailer": "^4.6.8",
    "nodemon": "1.18.4",
    "npm-run-all": "^4.1.3",
    "prisma": "1.17.1",
    "prisma-binding": "2.1.6",
    "stripe": "^6.12.1"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
